<table>
    <h1>Students</h1>
    <tr><a href="/">Home</a></tr>

        <tr>
            <th>Student ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Action</th>

        </tr>
        <% studentList.forEach((student) =>
        { %>
            <tr>
                <td><%= student.sid %></td>
                <td><%= student.name %></td>
                <td><%= student.age %></td>
                <td><a href="/">Update</a></td>

            </tr>
        <% }) %>
</table>
