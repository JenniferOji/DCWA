<table>
    <h1>Students</h1>
    <tr>
        <td><a href="/">Home</a></td>
    </tr>
    <tr></tr>
        <td><a href="/addStudent">Add Student</a></td>
    </tr>
        <tr>
            <th>Student ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Action</th>

        </tr>
        <% studentList.forEach((student) =>
        { %>
            <tr>
                <td><%= student.sid %></td>
                <td><%= student.name %></td>
                <td><%= student.age %></td>
                <td><a href="/updateStudent/<%= student.sid %>">Update</a></td>
            </tr>
        <% }) %>
</table>
